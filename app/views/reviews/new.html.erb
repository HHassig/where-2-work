form for the review with all the permited fields given in the reviews controller create method

<div>
  <%= simple_form_for [@booking, @review] do |f| %>
    <%= f.input :rating, as: :select, collection: ['1', '2', '3', '4', '5'] %>
    <%= f.input :title%>
    <%= f.input :comment%>
    <%= f.input :suited_for_calls%>
    <%= f.input :wifi_down%>
    <%= f.input :wifi_up%>
    <%= f.input :power_outlets%>
    <%= f.input :naural_light%>
    <%= f.input :coffe_price%>
    <%= f.input :food_price%>
    <%= f.input :photo, as: :file %>
    <%= f.submit class: 'btn btn-primary' %>
  <% end %>
  <%= link_to "See all Venues", venues_path %>
</div>
